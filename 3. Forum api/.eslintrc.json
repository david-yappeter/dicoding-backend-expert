{
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  },
  "env": {
    "node": true,
    "commonjs": true,
    "es6": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "camelcase": "off",
    "function-paren-newline": "off",
    "implicit-arrow-linebreak": "off",
    "object-curly-newline": "off",
    "import/order": "off",
    "import/no-dynamic-require": "off",
    "import/no-unresolved": "off",
    "global-require": "off",
    "no-unused-vars": "off",
    "no-console": "off",
    "class-methods-use-this": "off",
    "no-underscore-dangle": "off",
    "no-undef": "off",
    "comma-dangle": "off",
    "operator-linebreak": "off",
    "linebreak-style": "off"
  }
}
