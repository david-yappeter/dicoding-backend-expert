{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "function-paren-newline": "off",
        "implicit-arrow-linebreak": "off",
        "class-methods-use-this": "off",
        "no-underscore-dangle": "off",
        "linebreak-style": "off",
        "comma-dangle": "off"
    }
}
